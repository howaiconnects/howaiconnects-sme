          "id": "airtable-setup-webhook",
          "name": "Airtable Setup Trigger",
          "type": "n8n-nodes-base.webhook",
          "position": [240, 300]
        },
        {
          "parameters": {
            "url": "={{$env.FASTAPI_BASE_URL}}/api/v1/setup/initialize-airtable-schema",
            "sendHeaders": true,
