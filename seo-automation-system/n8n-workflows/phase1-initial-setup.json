        {
          "parameters": {
            "channel": "#seo-setup",
            "text": "âœ… Airtable Schema Setup Complete!\n\n**Base ID:** {{$runData['Airtable Setup Trigger'][0].json.body.base_id}}\n**Tables Created:** {{$json.tables_created}}\n**Setup ID:** {{$json.setup_id}}\n\n*Your SEO automation Airtable base is ready to use!*"
          },
          "id": "setup-complete-notification",
          "name": "Setup Complete Notification",
          "type": "n8n-nodes-base.slack",
          "position": [680, 300]
        }
      ],
      "connections": {
