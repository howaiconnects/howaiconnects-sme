                },
                {
                  "name": "content_type",
                  "value": "{{$runData['Get Content Details'][0].json.fields['Block Type']}}"
                },
                {
                  "name": "target_keywords",
                  "value": "={{$runData['Get SEO Context'][0]?.json?.records?.[0]?.fields?.['Target Keywords'] || []}}"
